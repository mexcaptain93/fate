extends ../layout/main

block variables
    - var title = 'Обратная связь'
    - var pageClass = 'feedback'

block content
    .container
        .breadcrumbs
            ul.breadcrumbs__list
                li.breadcrumbs__item
                    a(href='/index.html') Главная
                li.breadcrumbs__item
                    a(href='/category.html') Обратная связь
